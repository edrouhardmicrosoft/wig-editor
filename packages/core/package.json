{
  "name": "@wig/canvas-core",
  "version": "0.0.0",
  "type": "module",
  "description": "Shared types, protocol, and error definitions for canvas",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "import": "./dist/protocol/index.js"
    },
    "./transport": {
      "types": "./dist/transport/index.d.ts",
      "import": "./dist/transport/index.js"
    },
    "./version": {
      "types": "./dist/version/index.d.ts",
      "import": "./dist/version/index.js"
    },
    "./output": {
      "types": "./dist/output/index.d.ts",
      "import": "./dist/output/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ]
}
